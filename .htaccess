RewriteCond %{REQUEST_URI} !^/fontawesome/
RewriteRule !^es/ es%{REQUEST_URI} [L]

RewriteCond %{HTTP_HOST} ^movenonviolence.net$
RewriteCond %{REQUEST_URI} !^/assets/
RewriteCond %{REQUEST_URI} !^/fontawesome/
RewriteRule !^en/ en%{REQUEST_URI} [L]

