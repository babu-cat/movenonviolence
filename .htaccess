RewriteEngine On

##########
# NO WWW #
##########

RewriteCond %{HTTP_HOST} ^www\.(.*)$ [NC]
RewriteRule ^(.*)$ http://%1/$1 [R=301,L]

##########################################
# REDIRECCIONS MULTI-DOMINI MULTI-IDIOMA #
##########################################

RewriteCond %{HTTP_HOST} ^moulanoviolencia.net$
RewriteCond %{REQUEST_URI} !^/assets/
RewriteCond %{REQUEST_URI} !^/fontawesome/
RewriteRule !^ca/ ca%{REQUEST_URI} [L]

RewriteCond %{HTTP_HOST} ^muevelanoviolencia.net$
